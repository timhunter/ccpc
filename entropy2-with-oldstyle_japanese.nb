     "\[IndentingNewLine]", "\t\t\t", 
     RowBox[{"Scan", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"color", "[", 
