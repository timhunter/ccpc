     RowBox[{
      RowBox[{"color", "[", 
       RowBox[{"[", "u", "]"}], "]"}], " ", "=", " ", "Gray"}], ";", 
     "\[IndentingNewLine]", "\t\t\t", 
     RowBox[{"Scan", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"color", "[", 
              RowBox[{"[", "#", "]"}], "]"}], "\[Equal]", "White"}], ",", 
            "\[IndentingNewLine]", "\t\t\t\t", 
