       </diamond>
      </satop>
   </lf>
</macro>
<macro name="@n4">
  <fs id="0" attr="class" val="4"/>
   <lf>
     <satop nomvar="X">
       <diamond mode="Class">
         <prop name="3"/>
       </diamond>
      </satop>
   </lf>
</macro>
