  <entry stem="expressway" class="road" pos="N">
    <word form="expressway" macros="@sg @sg-X"/>
    <word form="expressways" macros="@pl @pl-X"/>
  </entry>
  <entry stem="road" class="road" pos="N">
    <word form="road" macros="@sg @sg-X"/>
